{
  "name": "dwbh",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:style": "vue-cli-service lint:style",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit --no-cache",
    "test:unit:coverage": "vue-cli-service test:unit --no-cache --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "luxon": "^1.11.4",
    "register-service-worker": "^1.5.2",
    "vue": "^2.5.22",
    "vue-class-component": "^6.0.0",
    "vue-i18n": "^8.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "^1.1.2",
    "@types/dotenv": "^6.1.0",
    "@types/faker": "^4.1.5",
    "@types/jest": "^23.1.4",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/luxon": "^1.11.1",
    "@types/webpack": "^4.4.0",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-e2e-cypress": "^3.4.0",
    "@vue/cli-plugin-pwa": "^3.4.0",
    "@vue/cli-plugin-typescript": "^3.4.0",
    "@vue/cli-plugin-unit-jest": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "coveralls": "^3.0.3",
    "faker": "^4.1.0",
    "lint-staged": "^8.1.0",
    "lodash.clonedeep": "^4.5.0",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.0.0",
    "ts-jest": "^23.0.0",
    "typescript": "^3.0.0",
    "vue-cli-plugin-i18n": "^0.5.2",
    "vue-cli-plugin-postcss-preset": "^1.0.2",
    "vue-cli-plugin-pug": "^1.0.7",
    "vue-template-compiler": "^2.5.21",
    "vuex-mock-store": "^0.0.7"
  },
  "gitHooks": {
    "pre-commit": "lint-staged && python scripts/add_license_to_src_files.py"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.{vue,htm,html,css,sss,less,scss}": [
      "vue-cli-service lint:style",
      "git add"
    ]
  }
}
